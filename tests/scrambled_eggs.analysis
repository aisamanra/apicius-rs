None: [
    [
        Join(
            "$mix",
        ),
        stir & [salt],
    ],
]
$mix: [
    [
        Ingredients(
            [
                [1/2]onion,
                [1 clove]garlic,
            ],
        ),
        chop coarsely,
        sautee & [butter],
    ],
    [
        Ingredients(
            [
                [2]eggs,
            ],
        ),
        whisk,
    ],
]
